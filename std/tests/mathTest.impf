import "./std/math";

fun testAbs(){
    assert(abs(-1) == 1);
    assert(abs(1) == 1);
    assert(abs(0) == 0);
    println("abs tests completed with no errors");
}

fun testMax(){
    assert(max(3,2) == 3);
    assert(max(2,2) == 2);
    assert(max(1,2) == 2);
    println("max tests completed with no errors");
}

fun testMin(){
    assert(min(3,2) == 2);
    assert(min(2,2) == 2);
    assert(min(1,2) == 1);
    println("min tests completed with no errors");
}



testAbs();
testMax();
testMin();