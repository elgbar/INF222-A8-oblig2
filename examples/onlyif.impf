var sign = fun(n){
    if(n < 0){
        return -1;
    } else if (n > 0){
        return 1;
    } else{
        return 0;
    }
    throw "Failed to sign";
};

var a = ref (-10);
while(*a <= 10){
    println("Sign of ", sign(*a));
    a = *a + 1;
}